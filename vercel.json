{
  "rewrites": [
    { "source": "/guias/editar/:id", "destination": "/guias/editar/[id]" },
    { "source": "/guias/ver/:id", "destination": "/guias/ver/[id]" },
    { "source": "/sacrificios/editar/:id", "destination": "/sacrificios/editar/[id]" },
    { "source": "/sacrificios/ver/:id", "destination": "/sacrificios/ver/[id]" },
    { "source": "/contactos/editar/:id", "destination": "/contactos/editar/[id]" },
    { "source": "/contactos/ver/:id", "destination": "/contactos/ver/[id]" },
    { "source": "/herramientas/reparar-guia/:id", "destination": "/herramientas/reparar-guia/[id]" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, max-age=0"
        }
      ]
    }
  ]
}
